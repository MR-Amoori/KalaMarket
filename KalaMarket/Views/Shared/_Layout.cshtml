<!DOCTYPE html>
<html lang="fa" dir="rtl">
<head>
    <meta charset="UTF-8">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>@ViewBag.Title</title>
    <!-- font---------------------------------------->
    <link rel="stylesheet" href="~/main/assets/css/vendor/font-awesome.min.css">
    <link rel="stylesheet" href="~/main/assets/css/vendor/materialdesignicons.css">
    <!-- plugin-------------------------------------->
    <link rel="stylesheet" href="~/main/assets/css/vendor/bootstrap.css">
    <link rel="stylesheet" href="~/main/assets/css/vendor/owl.carousel.min.css">
